{
  "agent": {
    "metrics_collection_interval": 60,
    "run_as_user": "root"
  },
  "metrics": {
    "namespace": "CWAgent",
    "metrics_collected": {
      "cpu": {
        "measurement": [
          "cpu_usage_idle",
          "cpu_usage_iowait",
          "cpu_usage_system",
          "cpu_usage_user"
        ],
        "metrics_collection_interval": 60
      },
      "disk": {
        "measurement": [
          "disk_io_time",
          "disk_io_time_weighted",
          "disk_used",
          "disk_free",
          "disk_inodes_free",
          "disk_inodes_used"
        ],
        "metrics_collection_interval": 60
      }
    }
  }
}
